        "courses.CreateCourseRequest": {
            "type": "object",
            "required": [
                "course_credit",
                "course_day",
                "course_department",
                "course_end_shift",
                "course_fullname",
                "course_name",
                "course_room",
                "course_semester",
                "course_start_shift",
                "course_teacher_id",
                "course_year",
                "max_enroll"
            ],
            "properties": {
                "course_credit": {
                    "type": "integer",
                    "maximum": 12,
                    "minimum": 1
                },
                "course_day": {
                    "enum": [
                        "monday",
                        "tuesday",
                        "wednesday",
                        "thursday",
                        "friday",
                        "saturday",
                        "sunday"
                    ],
                    "allOf": [
